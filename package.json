{
  "name": "coc-nerdfonts",
  "version": "1.2.0",
  "description": "Nerdfonts completion source of coc.nvim.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "coc": ">=0.0.57"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shadowwa/coc-nerdfonts.git"
  },
  "keywords": [
    "coc.nvim",
    "nerdfonts",
    "vim"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "coc.source.nerdfonts.enable": {
          "type": "boolean",
          "default": true
        },
        "coc.source.nerdfonts.shortcut": {
          "type": "string",
          "default": "T"
        },
        "coc.source.nerdfonts.triggerPatterns":{
          "type": "array",
          "default": [
            "nf-"
          ],
          "items": {
            "type": "string"
          }
        },
        "coc.source.nerdfonts.priority": {
          "type": "integer",
          "default": 3
        },
        "coc.source.nerdfonts.disableSyntaxes": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          }
        },
        "coc.source.nerdfonts.filetypes": {
          "type": [
            "array",
            "null"
          ],
          "default": [
            "markdown"
          ],
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "author": "Shad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shadowwa/coc-nerdfonts/issues"
  },
  "homepage": "https://github.com/shadowwa/coc-nerdfonts#readme",
  "devDependencies": {
    "coc.nvim": ">=0.0.57"
  }
}
